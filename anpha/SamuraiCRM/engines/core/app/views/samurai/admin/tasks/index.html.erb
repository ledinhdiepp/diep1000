<h2 class='pull-left'>Contacts</h2>
<%= render 'admin/shared/nav' %>
<hr>

<div class="panel panel-primary">
    <div class="panel-heading">
        All Contacts
    </div>

    <table class="table">
        <thead>
            <th>ID</th>
            <th>Title</th>
            <th>Content</th>
            <th>Create at</th>
        </thead>
        <tbody>
            <%- @tasks.each do |task| %>
                <tr>
                    <td><%= task.id %></td>
                    <td><%= task.title %></td>
                    <td><%= task.content %></td>
                    <td><%= task.created_at.strftime("%d %b. %Y") %></td>
                </tr>
            <% end %>
        </tbody>
    </table>
</div>